{
  "name": "Gandalf The Red",
  "description": "Chrome extension to ensure you understand what you agree to on the internet",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
